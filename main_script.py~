import os,sys
import generate_graph
import generate_W
import generate_abF
import generate_b

def usage():
    print "num_persons num_samples noise_strength num_nodes topology sub_nodes behav_ft"
    sys.exit(2)

def main():
    if len(sys.argv)<2:
        usage()
    
    num_persons=int(sys.argv[1])
    num_samples=int(sys.argv[2])
    noise_strength=int(sys.argv[3])
    num_nodes=int(sys.argv[4])
    topology=int(sys.argv[5])
    sub_nodes=int(sys.argv[6])
    behav_ft=int(sys.argv[7])
    generate_graph.main(num_nodes, topology, sub_nodes)
    generate_abF.main(num_ppl, num_samples, behav_ft)
    generate_b.main(num_ppl, behav_ft, noise_strength)
    generate_W.main(num_nodes, behav_ft)
    
if __name__ == '__main__':
    main()
